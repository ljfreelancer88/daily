{
    "name": "ljfreelancer88/daily",
    "description": "A PHP library for communicating with the Daily.co REST API.",
    "keywords": [
        "ljfreelancer88",
        "daily"
    ],
    "homepage": "https://github.com/ljfreelancer88/daily",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Jake Pucan",
            "email": "lj88@duck.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "vimeo/psalm": "^4.29"
    },
    "autoload": {
        "psr-4": {
            "Ljfreelancer88\\Daily\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ljfreelancer88\\Daily\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    }
}
